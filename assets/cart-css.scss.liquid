		.bill-field .form-group input[type=number] {-moz-appearance: textfield;}
		.bill-field .form-group .text-danger { margin-top: 5px; display: block;font-size: 11px; color: #ff0000;}
		.bill-field .form-btn .button{ height: 40px; padding-top: 0; padding-bottom: 0; line-height: 40px;}
	}
