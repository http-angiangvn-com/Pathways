/*===== CART===============================================*/
/* CSS icon load  */
@keyframes cart-ellipsis1 {0% {transform: scale(0); }100% { transform: scale(1); }}
@keyframes cart-ellipsis3 { 0% {transform: scale(1); }100% { transform: scale(0); }}
@keyframes cart-ellipsis2 { 0% { transform: translate(0, 0); } 100% { transform: translate(19px, 0); } }
.cart-ajloading { position: fixed;top: 0;left: 0;right: 0; bottom: 0; text-align: center; z-index: 9999; display: none;background: rgba(255, 255, 255, 0.3);}
.cart-ajloading > div:first-child { width: 100px;padding: 4px;position: relative;top: 50%; transform: translateY(-50%);margin: 0 auto;}
.cart-ajloading .lds-ellipsis { position: relative; width: 54px; height: 16px; margin: auto; }
.cart-ajloading .lds-ellipsis div {
	position: absolute; top: 6px;width: 6px; height: 6px; border-radius: 50%;
	background: var(--shop-color-main); animation-timing-function: cubic-bezier(0, 1, 1, 0);
}
.cart-ajxloading .lds-ellipsis div:nth-child(1) {left: 6px; animation: cart-ellipsis1 .6s infinite; }
.cart-ajloading .lds-ellipsis div:nth-child(2) { left: 6px;animation: cart-ellipsis2 .6s infinite; }
.cart-ajloading .lds-ellipsis div:nth-child(3) {left: 26px; animation: cart-ellipsis2 .6s infinite; }
.cart-ajloading .lds-ellipsis div:nth-child(4) {left: 45px; animation: cart-ellipsis3 .6s infinite; }
.wrapper-mainCart{
	margin: 40px 0;
	.mainCart-detail {background:#fff;margin-bottom: 50px;}
	.mainCart-detail .heading-cart {
		font-size: 20px;
		margin: 0;
		padding: 10px 0;
		border-bottom: 1px solid var(--shop-color-border);
		display: flex;
		flex-wrap: wrap;
		align-items: center;
		justify-content: space-between;
		h1 {font-size: 24px;margin:0 0 6px}
		&.heading-row {
			border-bottom: 0;
			.cart-shipping {
				flex: 0 0 100%;max-width: 100%;border-top: 1px solid var(--shop-color-border); 
				margin-top: 15px; padding-top: 15px;
			}
		}

	}
	.mainCart-detail .expanded-message {
		padding: 20px 0px;
	}
	.cart-shipping {
		padding: 10px 0 0;
		.price {font-size: 18px;font-weight: 600;color:#ff2c26;}
		.free-ship {
			text-transform: uppercase;
			color: #333333;
			font-weight: 600;
			@media(max-width: 767px){display: block;}
		}
		&__title {font-size: 16px; font-weight: 400;color: #666666;letter-spacing: 0.5px;}
		&__bar {
			height: 8px;
			width: 100%;
			background-color: #e9e9e9;
			border-radius: 5px;
			margin: 15px 0 10px;
			position: relative;
			.shipping-bar {
				background-color: #ffbc11;
				width: 0;
				border-radius: 5px;
				transition: width .4s ease-in-out;
				display: block;
				height: 100%;
				position: relative;
				.icon {
					width: 30px;
					height: 30px;
					display: inline-flex;
					align-items: center;
					justify-content: center;
					border-radius: 50%;
					position: absolute;
					top: 3px;
					right: 10px;
					transform: translate(50%, -50%);
					font-weight: 700;
					content: "";
					font-size: 12px;
					color: #ffbc11;
					border: 1px solid #ffbc11;
					background: currentColor;
					svg {fill: #fff;}
				}
			}
		}
		&.cart-shipping-free {
			.cart-shipping__bar span {background-color:#3d9851;}
			.cart-shipping__bar .icon {
				background-color: #3d9851;
				border-color: #3d9851;
				color: #3d9851;
			}
		}
	}
	.collectionCart-detail {
		margin: 0 0 40px;
		.collectionCart-title {font-size: 20px;font-weight: bold;margin-top: 10px;margin-bottom: 15px;}
		.list-product {
			display: flex;flex-wrap: wrap;
			.product-loop-items {padding: 0 12px;}		
			.slick-list {margin: 0 -12px;}
			.slick-arrow{
				position: absolute;	top: 30%; z-index: 8;
				-webkit-transform: translateY(-50%);	-moz-transform: translateY(-50%);
				-ms-transform: translateY(-50%);-o-transform: translateY(-50%);transform: translateY(-50%);
				padding: 0;	margin: 0px;font-size: 0;color:var(--shop-color-text);
				height: 35px;width: 35px;line-height: 35px;
				outline: none;text-align: center;display: inline-block;
				background: transparent;border:0;box-shadow: none;
				background: rgba(0,0,0,0.08);
				border-radius: 2px;
				&:before{
					content: "";
					font-family: "Font Awesome 5 Pro";
					font-size: 28px;display: inline-block;
				}
				&.slick-prev{left: 0px;}
				&.slick-next{right: 0px;}
				&.slick-next:before {content: "\f105";}
				&.slick-prev:before {content: "\f104";}
				&:hover{}
				&.slick-disabled{opacity:0;visibility: hidden;}
			}
			&:not(.slick-initialized) {display: -webkit-box;display: -ms-flexbox;display: flex;flex-wrap: nowrap;overflow: hidden;}
			&:not(.slick-initialized) .product-loop-items {flex: 0 0 25%;max-width: 25%;}
		}
	}
	.expanded-content {
		.expanded-info {text-align: center;}
		.info-text .text1 {font-size: 26px;font-weight: 500;margin-bottom: 10px;}
		.info-text .text2 {font-size: 18px;margin-bottom: 20px;font-weight: 500;color: #606876;}
		.info-text .text2 a {color: #1a73e8;font-weight: 600;}
		.search-field {
			position: relative;
			.search_box {
				height: 45px;width: 100%;
				padding: 8px 50px 8px 20px;
				border: 4px solid #e3e9ed;border-radius: 4px;font-size: 15px;
				font-weight: 500;
				&:focus {outline: none;}
			}
			input[type="submit"] {width: 45px;height: 45px;position: absolute;right: 0;}
			.btn-search {
				width: 50px;
				height: 44px;
				line-height: 42px;
				position: absolute;
				padding: 0;
				top: 0px;
				right: 0px;
				background: transparent;
				transition: opacity 150ms linear;
				outline: none;
				box-shadow: none;
				border: none;
				display: flex;
				align-items: center;
				justify-content: center;
				svg {width: 24px;height: 100%;display: block;margin: auto;fill:#6c798f;opacity: 0.6;}
			}
		}
	}
	.title-number-cart {padding:0;font-weight: 400;font-size: 16px;margin:0 0 6px;}
	.list-pageform-cart {padding: 15px 0;}
	.table-cart {
		padding: 8px 10px;
		border: 2px solid var(--shop-color-border);
		overflow: hidden;
		border-radius: 8px;
	}
	.cart-row {width: 100%;display: inline-block;}
	.expanded-message {
		@media(min-width:768px){
			&{text-align:left;font-size: 17px;}
		}	
	}

	.media-line-item{
		position: relative;
		display: -ms-flexbox;display: -webkit-flex;display: flex;
		-webkit-flex-direction: row;	-ms-flex-direction: row;	flex-direction: row;
		-webkit-justify-content: space-between;justify-content: space-between;padding: 15px 10px;
		.media-left{
			-ms-flex: 0 0 80px;	flex: 0 0 80px;max-width: 80px;
			padding: 0;position: relative;	display: block;
			img{border: 1px solid #ededed;}
			.item-remove {position: absolute;	top: -7px;left: -10px;}
			.item-remove a {
				display: block;
				background: #8f9bb3;
				color: #fff;
				font-size: 8px;
				font-weight: 400;
				width: 20px;
				height: 20px;
				line-height: 20px;
				text-align: center;
				border-radius: 50%;
			}
		}
		.media-right{
			-ms-flex: 1 1 auto;flex: 1 1 auto;
			padding-left: 18px;padding-right: 18px;
		}
		.media-total {text-align: right;}
		.item-info{margin-bottom:12px;}
		.item-info .item--title{font-size: 16px;margin: 0 0 10px;font-weight: 500;}
		.item-info .item--title a {
			color: var(--shop-color-text);overflow: hidden;
			-webkit-line-clamp: 2;	-webkit-box-orient: vertical;display: -webkit-box;
		}
		.item-variant{
			font-size: 12px;color: #777777;font-weight: 500;margin:0;    
			display: -webkit-flex;  display: flex;width:100%;padding-right:5px;
			span{overflow: hidden; -webkit-line-clamp: 1;  -webkit-box-orient: vertical; display: -webkit-box;}
			&-edit{
				cursor: pointer;   
				span{ 
					position: relative;display: inline-block;border-radius: 3px;
					padding: 2px 25px 2px 9px;color: #5c5c5c;background:#f5f5f9;
					-webkit-line-clamp: 1;  -webkit-box-orient: vertical; display: -webkit-box;
				}
				span:after{
					content:"";
					display: inline-block;	vertical-align: middle;
					border-left: 4px solid transparent;
					border-right: 4px solid transparent;
					border-top: 5px solid rgba(0,0,0,.54);border-bottom: 0;
					background: none;padding: 0;
					position: absolute;top: 50%;right: 6px;
					-ms-transform: translateY(-50%);transform: translateY(-50%);
				}
			}
		}

		.item-prmote{margin:2px 0;font-style: italic;font-size: 13px;color: #ab1207;}
		.item-price p {margin:0;}
		.item-price .ico-gift {
			display: inline-block;
			max-width: 18px;
			vertical-align: 2px;
			margin-left: 10px;
		}
		.item-price span {font-weight: 500;color: #8f9bb3;}
		.item-price del {color: #8f9bb3;font-size:12px;margin-left: 5px;}
		.item-qty {display: inline-block;}
		.item-qty .quantity-partent{
			margin: 8px 0 3px;
			display: flex;-ms-display: flex;-webkit-display: flex;
			width: 100%;
		}
		.item-qty .quantity-partent input.item-quantity {
			float: left;font-weight: 500;
			font-size: 15px;width: 36px;height: 28px;padding: 0;
			background: #fff;text-align: center;outline: none;
			border: 0;border: 1px solid #f9f9f9;
			-webkit-appearance: none;-moz-appearance: none;-o-appearance: none;appearance: none;		
		}
		.item-qty .quantity-partent button.qty-btn {
			float: left;font-weight: 500;
			font-size: 18px;padding: 0;height: 28px;width: 28px;
			text-align: center;background: #f9f9f9;color:var(--shop-color-text);
			border: 1px solid #f9f9f9;outline: none;
			-webkit-appearance: none;-moz-appearance: none;-o-appearance: none;appearance: none;	
			display: flex;
			align-items: center;
			justify-content: center;
			svg {
				display: block;
				width: 10px;
				height: 100%;
				margin: 0 auto;
			}
		}
		.item-qty .quantity-partent button.qty-btn:focus {outline: none;}
		.item-total-price .text {font-size:14px;}
		.item-total-price .line-item-total{font-size: 16px;font-weight:700;}	
	}
	.media-line-item + .media-line-item {border-top: 1px solid #f1f1f1;}

	.order-noted-block .checkout-buttons{	background: #f3f4f4; padding: 15px; margin: 20px 0 10px;	}
	.order-noted-block label.note-label { display: block;margin:00px 0 10px;font-weight: 600; font-size: 14px;}
	.order-noted-block textarea {padding:10px 15px;width: 100%; min-height: 80px; resize: none;	border: 1px solid #dfe0e1;box-shadow:none;}
	.order-summary-block{
		background:#fff;padding: 15px;margin-bottom:15px;border: 1px solid var(--shop-color-border);
		&.order-summary-notify{padding:0;border:0;}
		.summary-warning{
			padding: 12px 15px;border-radius: 4px;
			/*	background-color: #fee3e8;border:1px solid #fdd0d8;*/
			background-color: #d9edf7;  border:1px solid #bce8f1;
			.textmr{ margin-bottom: 5px;}
			p{margin:0;}

		}
		.summary-alert{
			background-color: #fee3e8; border: 1px solid #fdd0d8; color: #d20909;
			font-size: 13px;  padding:10px 15px;margin: 10px 0;text-align: left;
			&:not(.in){display: none;}
		}
		.summary-title {font-size: 20px;font-weight: bold;margin-top: 10px;margin-bottom: 15px;}
		.summary-time{
			background: #fcfcfc;
			border: 1px solid #F5f5f5;
			&__row{
				padding: 10px 8px;
				display: -ms-flexbox; display: flex;
				-ms-flex-wrap: wrap; flex-wrap: wrap;
				-ms-flex-pack: justify;justify-content: space-between;
				.boxtime-title {
					-ms-flex: 1 1 auto;	flex: 1 1 auto;	width: 1%;padding-right:10px;
					.txt-title{text-transform: uppercase;font-size: 13px;font-weight: 400;margin:0 0 5px;}
					.txt-time{
						font-size:13px;font-weight:600;margin:0;
						i{color:var(--shop-color-main);margin-right:2px;font-size: 15px;    vertical-align:middle;}
					}
				}
				.boxtime-radio{
					max-width:50%;
					.radio-item{position: relative;}
					.radio-item + .radio-item{margin-top:5px;}					
					input[type='radio']{
						position: absolute;	top: 2px;left: 0;
						height:15px;width: 15px; border-radius: 50%;
						color: #fff;background: #ffffff;	border:1px solid #e5e5e5;
						cursor: pointer;outline: none;display: inline-block;margin: 0;
						-webkit-transition: all 0.15s ease-out 0s;transition: all 0.15s ease-out 0s;
						-webkit-appearance: none;	-moz-appearance: none;-ms-appearance: none;-o-appearance: none;appearance: none; 
					}
					input[type='radio'] + label{				
						position: relative;margin:0;				
						padding-left: 20px;	font-size: 13px;font-weight:500;cursor: pointer; 
					}
					input[type='radio']:before{
						content: '\2714\fe0e';font-size: 11px;
						height: 13px;	width: 13px;line-height: 13px;opacity: 0;
						position: absolute;display:block;	text-align: center;
					}
					input[type='radio']:checked{background:var(--shop-color-hover);border-color:var(--shop-color-hover);}					
					input[type='radio']:checked:before{opacity: 1;color: #ffffff; }
				}
				.boxtime-select{
					-ms-flex: 0 0 100%;flex: 0 0 100%; max-width: 100%;
					.select-choose{
						margin-right: -5px; margin-left: -5px;
						display: -ms-flexbox; display: flex;
						.select-box {
							-ms-flex-preferred-size: 0;	flex-basis: 0;-ms-flex-positive: 1;
							flex-grow: 1;min-width: 0;	max-width:50%;
							padding-left:5px;padding-right:5px;	margin-bottom:10px;
							.select-option{
								position: relative;
								&:after {
									content: '';
									position: absolute;	top: calc(50% - 5px);
									right: 10px;border: solid #666666;
									border-width: 0px 1px 1px 0;
									display: inline-block;padding: 3px;
									transform: rotate(45deg);-webkit-transform: rotate(45deg); 
								}
							}
							label {display: block;font-weight: 400; font-size: 13px;}
							select {
								padding:0px 8px;padding-right:20px;
								cursor: pointer;outline: none;border-radius:4px;
								height: 34px;line-height: 32px;background:#ffffff;border: 1px solid #e5e5e5;
								font-size: 14px;width: 100%;color: #000;font-weight: 500;								
								-webkit-user-select: none; -moz-user-select: none;  -ms-user-select: none;  user-select: none;
								-webkit-appearance: none; -moz-appearance: none; -ms-appearance: none; -o-appearance: none;  appearance: none;
								&:-ms-expand {display: none;}							
							}
							select:invalid {color: #ededed; }
							select [disabled] {color: #ededed;font-weight: 300; }
							select option {color: #000;font-weight: 500; }
						}
					}
					.select-button .btn-accepttime{
						display: block;width: 100%;	border-radius: 4px;
						padding: 0 10px;height: 34px;line-height: 30px;
						color:var(--shop-color-main);background: #ffffff;border: 2px solid var(--shop-color-main);
						font-size: 13px;font-weight: 500;text-transform: uppercase;		
						&.disabled{opacity:0.8}
					}
				}
				&.picktime_selecter{display:none;border-top: 1px solid #efefef;}
			}
		}
		.summary-time + .summary-total{border-top:0;}
		.summary-total {
			font-size: 16px;padding-bottom: 10px;padding-top: 10px;font-weight: bold;
			border-top: 1px dotted #dfe0e1;border-bottom: 1px dotted #dfe0e1;margin-bottom:10px;
			p{display: flex; align-items: center; justify-content: space-between;margin:0;}
		}
		.summary-total p span {font-weight: bold;float: right;color:red;font-size: 24px;}	
		.summary-action {padding-top:5px;text-align: center;}
		.summary-action p {
			position: relative;font-size: 14px;margin-bottom:4px;padding-left: 15px;font-weight: 400;text-align: left;
			&:before {
				content: "";
				width: 4px;
				height: 4px;
				background: #999999;
				left: 0;
				opacity: 1;
				position: absolute;
				top: 8px;
				border-radius: 50%;	
			}

		}
		.summary-button .checkout-btn {
			display: block;
			background: red;
			color: #fff;
			text-decoration: none;
			text-transform: uppercase;
			padding: 10px 5px;
			text-align: center;
			font-size: 15px;
			font-weight: bold;
			border-radius: 0;margin-top:12px;
			&.disabled{pointer-events: none; opacity: 0.7;background: #151515;}
		}
		.cart-coupon {
			position: relative;
			.list-coupon {padding-bottom: 0;}
			.swiper-nav {
				position: absolute;
				top: 0;
				right: 0;
			}
			.swiper-button.swiper-next {margin-left: 5px;}
			.swiper-button.swiper-button-disabled svg{fill: #858380;cursor: no-drop;}
		}
	}
	.txt-continue {margin:15px 0;}
	.order-noted-block .btn-checkout.disabled{pointer-events: none; opacity: 0.7;background: #151515;}
	.order-invoice-block {
		.checkbox{margin:5px 0; position: relative;}
		.checkbox > .title {
			font-size: 14px;padding-left:40px;
			vertical-align: text-top;	margin-bottom: 0;display: block;font-weight: bold;
		}
		.checkbox .regular-checkbox {display: none; }
