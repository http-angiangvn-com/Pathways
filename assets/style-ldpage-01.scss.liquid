@media(max-width:767px){
	.modal-coupon--backdrop{
		position: fixed;	background-color: rgba(0, 0, 0, 0.5); top: 0; left:0;right:0;height:100%;z-index:999;
		display:none;opacity: 0;visibility: hidden;transition: all 0.3s cubic-bezier(0.645, 0.045, 0.355, 1);
	}
	.modal-coupon--backdrop.js-modal-show {	display:block;opacity: 1;visibility: visible;pointer-events: auto;}
}
/*========  Product-loop ==========*/
{%- assign productAspectCheck = settings.prod_resize_img | lstrip | rstrip | times: 1  -%}
{%- assign productAspect = '100%' -%}

{%- if productAspectCheck != blank -%}
{%- assign productAspect = productAspectCheck | append: '%' -%}
{%- endif -%}

.product-loop-ldpage{
	&{position:relative;padding-left:7px;padding-right:7px;margin-bottom:14px;}
	.product-block {
		background:#fff;height:100%;
		display: -ms-flexbox;display: -webkit-flex;display: flex;
		-webkit-flex-direction: column;-ms-flex-direction: column;flex-direction: column;justify-content: space-between;
		box-shadow: 0 -1px 6px rgba(0, 0, 0, 0.12);
		border-radius: 4px;
	}
	.proloop-image{
		position: relative;overflow: hidden;padding:7px;
		.pro-sale,.pro-soldout{
			position: absolute;top:7px;z-index:3;
			color: #fff;font-size: 12px;font-weight: 500;padding:2px 5px;
		}
		.pro-soldout{left:7px;background:#565656;font-size: 11px;padding-top:3px;padding-bottom:3px;}
		.pro-sale{left:7px;background: red;letter-spacing: 0.5px;}	
		.pro-sale + .pro-soldout{top:32px}
		.product--image {
			position: relative;
			.lazy-img.second-image {
				-webkit-opacity: 0; -moz-opacity: 0;  opacity: 0;
				position: absolute;	width: 100%;height: 100%;top: 0;bottom:0;
				transform: translate(100%, 0);
			}
		}
		.proloop-link {
			position: absolute;
			top: 0;
			left: 0;
			width: 100%;
			height: 100%;
			z-index: 2; }
		.gift.product_gift_label{
			width: 30px;
			height: 30px;
			line-height: 25px;
			position: absolute;
			top: 5px;
			right: 5px;
			z-index: 1;
			text-align: center;
			border-radius: 50%;
			padding: 0 5px;
			background: #ffffff;
			border: 1px solid var(--shop-color-border);
		}
		.sticker_frame {
			position: absolute;
			left: 0;
			right: 0;
			bottom: 0;
			z-index: 99;
		}
		.prod-label-onsale {
			position: absolute;
			left: 0;
			width: 100%;
			z-index: 2;
			bottom: 0;
		}
		.icon-quickview {
			font-family: 'FontAwesome';
			content: "\f06e";
			position: absolute;
			top: 50%;
			left: 50%;
			font-size: 18px;
			transform: translate(-50%, -50%);
			z-index: 995;
			width: 34px;
			height: 34px;
			line-height: 31px;
			border-radius: 50%;
			border: 1px solid #eee;
			text-align: center;
			background: #fff;
			color: #6d6565;
			opacity: 0;
			-webkit-transition: all 400ms ease;-ms-transition: all 400ms ease;transition: all 400ms ease;
			&:hover{background-color: #444;color:#fff;border:1px solid #fff;}
		}
		.lazy-img {
			position: relative;
			height: 0;
			width: 100%;
			padding-bottom: {{productAspect}};
			-webkit-transition: all 500ms ease;
			-moz-transition: all 500ms ease;  -ms-transition: all 500ms ease;
			-o-transition: all 500ms ease;   transition: all 500ms ease;
			img {
				position: absolute;
				top: 50%;
				left: 50%;
				display: block;
				object-fit: contain;
				transform: translate(-50%, -50%);
				-ms-transition: all 500ms ease;  -o-transition: all 500ms ease;  transition: all 500ms ease;
				max-height: 100%;
			}
		}
	}
	.proloop-detail{
		position:relative;padding: 10px 14px 10px;
		display: -ms-flexbox;display: -webkit-flex;display: flex;
		-webkit-flex-direction: column;-ms-flex-direction: column;flex-direction: column;
		-webkit-flex: 1 1 auto;-ms-flex: 1 1 auto;flex: 1 1 auto;
	}
	.proloop--review {
		margin-bottom:6px;
		.haravan-product-reviews-badge {
			overflow: hidden;
			display: -webkit-flex; display: flex;
			-webkit-align-items: center;align-items: center;
			-ms-justify-content: center;justify-content: center;
			.hrv-product-reviews-star{	
				display: -webkit-flex; display: flex;
				-webkit-align-items: center;align-items: center;
				i[class^="star-"] {color: #fdd836!important;font-size: 13px;margin-right: -3px;display:inline-block}
				i.star-off-png{color: #dadada !important; }  
			}
			.hrv-product-reviews-star + span,.hrv-product-reviews-star + p{margin-bottom:0;margin-left:6px;color: #637381;}					
			.hrv-product-reviews-star + p{
				margin-bottom:0;margin-left:8px;font-size: 0;visibility: hidden;
				&:before{content:"(0)";display: inline-block;visibility: visible;}
			}	
			.hrv-product-reviews-star + span,.hrv-product-reviews-star + p:before{font-size: 12px;}
		}
	}
	.proloop--review + .proloop--action{margin-top:0;}
	&:not(.product-horizontal) {
		.pro-percent {display: none;}
		.prohzt-soldout {display: none;}
	}
	&.product-horizontal {
		padding-right: 0;padding-left: 0;margin-bottom: 0;
		.product-inner {
			display: -ms-flex;display: flex;
			-ms-flex-direction: row;flex-direction: row;
			box-shadow: none;
		}
		.proloop-image {display:block;min-width:100px;padding: 10px;}
		.proloop-image .icon-quickview {display:none;}
		.pro-soldout {display: none;}
		.proloop-detail {
			padding-left: 15px;padding-right: 0;
			text-align: left;
			.proloop-actions {display: none;}
		}
		.pro-sale {display: none;}
		.pro-percent {
			display: inline-block;
			padding: 2px 5px;
			margin-left: 4px;
			background: #fff;
			border-radius: 3px;
			text-transform: uppercase;
			font-weight: 600;
			font-size: 10px;
			color: #ff0000;
			border: 1px solid #ff0000;
			vertical-align: text-bottom;
		}
		.prohzt-soldout {
			display: block;
			font-size: 13px;font-weight: 500;
			color: #878c8f;
		}
		&:hover .product-inner {box-shadow: none;}
	}
	@media(max-width:1199px) and (min-width:992px){
		.proloop-detail {padding: 5px 7px 5px;}
		&.col-lg5 .proloop-actions .proloop-actions__inner .actions-primary .btn-proloop-cart {padding: 0 35px 0 5px;width: 100%;font-size: 10px;}
	}
	@media(min-width:992px){
		.product-block{
			&{
				position: relative; overflow: hidden;
				-webkit-transition: all 400ms ease; -moz-transition: all 400ms ease;
				-ms-transition: all 400ms ease;  -o-transition: all 400ms ease;  transition: all 400ms ease;
			}
			.proloop-detail{
				background: #fff;
				-webkit-transition: transform 300ms ease; -moz-transition: transform 300ms ease;
				-ms-transition: transform 300ms ease;  -o-transition: transform 300ms ease;  transition: transform 300ms ease;
			}		
			&:hover{
				-webkit-box-shadow: 0 2px 12px rgba(0, 0, 0, 0.12);
				box-shadow: 0 2px 12px rgba(0, 0, 0, 0.12);
				.proloop-image{
					.product--image {
						.lazy-img.first-image {-webkit-opacity: 0; -moz-opacity: 0;opacity: 0;transform: translate(-100%, 0);}
						.lazy-img.second-image {-webkit-opacity: 1; -moz-opacity: 1;opacity: 1;transform: translate(0, 0);}
					}
					.icon-quickview {opacity: 1;}
				}
			}
		}
	}
	@media(max-width:991px){
		.proloop-image .icon-quickview {display:none;}
		.proloop-actions {
			position: initial;
			visibility: visible;
			.proloop-actions__inner .btn-proloop-cart .btnadd:hover:after {display:none;}
		}
		&.product-horizontal {
			.proloop-image {padding: 5px; min-width: 90px;}
			.proloop-detail {padding-left: 5px;}
			.proloop-detail .proloop--price {padding-bottom: 0;}
		}
	}
	@media(max-width:767px){
		.proloop-detail{
			&{padding:7px 10px 7px;}
		}
		&.product-horizontal {
			.proloop-detail {padding-left: 10px;}
		}
	}
	@media(max-width:480px){		
		.proloop-image .product--image .prod-img img{width:100%}
	
	}
}
.listProduct-ldpage-row{margin-left:-8px;margin-right:-8px;}
@media(min-width:992px){		
	.product-loop-ldpage.col-lg-5{flex:0 0 20%;-ms-flex:0 0 20%;max-width:20%;padding: 7px;}		
}
@media(max-width:767px){
	.listProduct-ldpage-row{margin-left:-4px;margin-right:-4px;}
	.product-loop-ldpage{padding-left:4px;padding-right:4px;margin-bottom:8px;}
}

/*========= ldp-book-01 ==========*/
.layoutPage-ldpage01{	
	background:var(--ldpage01-color-bg);
	margin-bottom: -30px;
	&.paddingTop{padding-top:40px}
	.product-loadding {
		.product-loop-load {border: 0; margin:0;}
		.loading-mask__image {padding: 7px 7px 0;}
		.loading-mask__image .mask-line {border-top-left-radius: 7px;border-top-right-radius: 7px;}
		.loading-mask__detail {padding: 7px;}
	}
	.section-ldpage-heading {
		.ldpage-hTitle{
			overflow: hidden;margin-bottom:15px;
			h1 {
				font-size:40px; line-height: 1.4; margin: 0; font-weight:600;
				color:var(--ldpage01-color-main);background: transparent;position: relative;padding: 0 25px;
			}
			.subtitle{margin:10px 0 0;font-weight: 400;}
			@media(min-width:768px){
				h1{
					display: inline-block;max-width: calc(100% - 400px);
					&:before, &:after {
						content: "";					
						width: 200px;height: 2px;
						position: absolute;	top: 50%;
					}
					&:before {
						right: 100%;
						background-image: linear-gradient(to right, transparent, var(--ldpage01-color-main));
						-webkit-transform: translateY(-50%);transform: translateY(-50%);
					}
					&:after {
						left: 100%;
						background-image: linear-gradient(to right, var(--ldpage01-color-main), transparent);
						-webkit-transform: translateY(-50%);transform: translateY(-50%);
					}
				}
			}
			@media(max-width:991px) and (min-width:768px){
				h1{
					font-size:30px;max-width: calc(100% - 280px);
					&:before, &:after {width:120px;}
				}
