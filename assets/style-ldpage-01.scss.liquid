/*======== coupon -- popover ==========*/
.coupon-popover{			
	margin-left: 15px;
	margin-right: 15px;
	max-width: 400px;
	font-family: inherit;
	font-size: 13px;
	z-index: 500;
	padding: 0;
	border: 0;
	border-radius: 8px;
	background: #ffffff;
	box-shadow: rgba(0, 0, 0, 0.15) 0px 1px 18px;
	.popover-header{
		display:none;		
		text-align: center; background: #ffffff; border-radius: 16px 16px 0 0; 
