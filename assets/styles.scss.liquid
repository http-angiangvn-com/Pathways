	&__icons{
		position: fixed;top: auto !important;bottom:105px;right:18px;margin: 0;z-index: 1000; cursor: pointer;
		.box-item{
			position: relative;
			& + .box-item {margin-top:18px;}
			.svgico {
				position: relative; z-index: 4;    display: block;
				height:48px;width:48px;text-align: center;border-radius: 50%;
				border:1px solid #ffffff;    cursor: pointer;
				background:var(--shop-color-main); box-shadow: 0 4px 8px rgba(0, 0, 0, 0.15);	
				box-sizing: border-box; -moz-box-sizing: border-box; -webkit-box-sizing: border-box;
				animation: pulse 2s infinite;animation: pulse 2s infinite; 
