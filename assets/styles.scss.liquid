/* 2.5 - CSS Loading  */
.animated-background {
	animation-duration: 1.25s;
	animation-fill-mode: forwards;
	animation-iteration-count: infinite;
	animation-name: placeHolderShimmer;
	animation-timing-function: linear;background: #F6F6F6;
	background: linear-gradient(to right, #F6F6F6 8%, #F0F0F0 18%, #F6F6F6 33%);
	background-size: 800px 104px; position: relative;
}
.loading-mask .loading-mask__image {
	position: relative; overflow: hidden;width: 100%;
	.mask-line{
		@extend .animated-background;
	}
}
.loading-mask .loading-mask__detail {padding:10px;}
.loading-mask .loading-mask__detail .mask-line {
	height: 10px;width: 100%;border-radius:8px;margin: 4px 0;
	@extend .animated-background;
	&:nth-child(2){width:80%}
	&:nth-child(3){width:65%}
}
