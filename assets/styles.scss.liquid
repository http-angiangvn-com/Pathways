.owlCarousel-style{	
	&.owl-carousel .owl-nav .owl-prev,
	&.owl-carousel .owl-nav .owl-next {
		position: absolute;top:-30px; z-index: 3;
		width: 30px; height: 30px; line-height: 28px;  font-size: 14px; border-radius: 50%;
		color: #858380;background: #fff;border: 1px solid var(--shop-color-border); outline: none;
		-webkit-box-shadow: 0 0.125rem 0.25rem rgba(0, 0, 0, 0.075); box-shadow: 0 0.125rem 0.25rem rgba(0, 0, 0, 0.075);
		-webkit-transition: all 0.4s;-ms-transition: all 0.4s; transition: all 0.4s;	
		span{
			font-size: 0;
			&:before{ content: ""; font-family: 'FontAwesome'; font-size: 22px; 
			}
		}
	}
	&.owl-carousel .owl-nav .owl-prev{
		right: 47px;
		span:before {content: "\f104"; }
	}
	&.owl-carousel .owl-nav .owl-next{
		right: 7px;
		span:before { content: "\f105"; }
	}
	&.owl-carousel .owl-nav .owl-prev.disabled,
	&.owl-carousel .owl-nav .owl-next.disabled{
		opacity: 0.5;
		&:hover {background: #fff;color: #858380;cursor: no-drop;}
	}
	&.owl-carousel:not(.owl-loaded) .slider-item {-webkit-flex: 0 0 100%; flex: 0 0 100%;}
	/*&.owl-carousel .owl-stage {margin: 5px 0;display: flex;display: -ms-flex;}*/
	.owl-dots{
		position: absolute; left: 0;right:0;bottom:-50px;text-align: center;
		.owl-dot {
			&{margin: 0 3px;outline:0;} 
			span {
				display: flex;display: -ms-flex;display: -webkit-flex;
				width: 7px;	height: 7px;margin: 5px;
				border-radius: 50%;
				background: #928c8c;
			}
			&.active {
				border-radius: 50%;
				border: 2px solid #000000;
			}
			&.active span {
				background: #000000;
			}
		}
	}
	.product-loop .product-inner {margin-bottom: 0;}

	@media(min-width:992px) and (max-width:1199px){
		&.owl-carousel .owl-nav .owl-prev{right: 47px;}
		&.owl-carousel .owl-nav .owl-next{right: 7px;}
	}
	@media(min-width:992px){		
		&.owl-carousel .owl-nav .owl-prev:hover,
		&.owl-carousel .owl-nav .owl-next:hover {background:var(--shop-color-button);color:#fff;}
	}	
	@media(max-width:767px){
		.owl-item:hover {box-shadow: none;}
		&.owl-carousel .owl-nav .owl-prev, &.owl-carousel .owl-nav .owl-next {
			top: -25px;
			span:before {font-size: 18px;}
		}
		&.owl-carousel .owl-nav .owl-prev{right: 45px;}
		&.owl-carousel .owl-nav .owl-next{right: 7px;}
		.owl-dots .owl-dot span {width: 5px; height: 5px;margin: 4px;}
		.owl-dots {bottom:-30px;}
	}
}
.banner-hover-effect {
	position: relative;overflow: hidden;display: block;
	&:before, &::after {
		content: "";
		position: absolute;
		height: 0;
		width: 0;
		background-color: rgba(255, 255, 255, 0.15);
		transition: all .5s ease-in-out 0s;
		-webkit-transition: all .5s ease-in-out 0s;
		-ms-transition: all .5s ease-in-out 0s;
	}
	&:before {
		top: 0;
		left: 0;
		z-index: 1;
	}
	&:after {
		bottom: 0;
		right: 0;
		z-index: 2;
	}
	@media (min-width: 1200px){
		&:hover:before, &:hover:after {
			width: 100%;
			height: 100%;
		}
		img {
			display: block;
			margin: 0;
			height: auto;
			transition: all 0.5s ease-in-out;
			-moz-transition: all 0.5s ease-in-out;
			-webkit-transition: all 0.5s ease-in-out;
			-o-transition: all 0.5s ease-in-out;
		}
		&:hover img {
			-moz-transform: scale(1.1);
			-webkit-transform: scale(1.1);
			-o-transform: scale(1.1);
			-ms-transform: scale(1.1);
			transform: scale(1.1);
		}
	}
}
.flow-zoom {
	overflow: hidden;
	img {transition: all 400ms ease;-ms-transition: all 400ms ease;-webkit-transition: all 400ms ease;}
	&:hover img {transform: scale(1.15);-ms-transform: scale(1.15);-webkit-transform: scale(1.15);}
}
.tooltip-cs{
	position: relative;
	.tooltip-hover{
		position: absolute;
		top: -36px;
		left: 50%;
		transform: translateX(-50%);
		height: 24px;
		line-height: 24px;
		background: #17191a;
		color: #fff;
		font-size: 10px;
		text-decoration: none;
		white-space: nowrap;
		padding: 0 8px;
		border-radius: 3px;
		z-index: 999;
		opacity: 0;
		visibility: hidden;
		transition: all 300ms ease;
		&:after{
			content: "";
			display: block;
			position: absolute;
			bottom: 1px;
			left: 50%;
			width: 0;
			height: 0;
			border: solid;
			border-width: 8px 8px 0 8px;
			border-color: transparent;
			border-top-color: #000;
			-webkit-transform: translate(-50%, 100%);
			transform: translate(-50%, 100%);
		}
	}
	&:hover .tooltip-hover{
		opacity: 1;
		visibility: visible;
	}
}
