			-ms-transition-delay:0.1s;;-webkit-transition-delay: 0.15s;transition-delay: 0.1s;
			transform: scale(1); -webkit-transform: scale(1); -moz-transform: scale(1);	-ms-transform: scale(1); -o-transform: scale(1);
