				&:before, &:after{display:none;}
				.svgico {background: #ffffff;padding:3px;box-shadow: 0 3px 12px rgba(0, 0, 0, .15);transition: box-shadow 150ms linear;}
				.svgico svg{ width: 100%; height: 100%;}
