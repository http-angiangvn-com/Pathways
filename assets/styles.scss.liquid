/* 2.2 - Breadcrumb */  
.breadcrumb-shop{
	background:var(--shop-color-bg);
	padding:0 0px;
	border-bottom: 1px solid #dfe2e6;
	border-top: 1px solid #dfe2e6;
}
.breadcrumb-paralax{
	position: relative;overflow: hidden;text-align: center;padding:0px 0;margin-bottom: 40px;
	background-color:#fafafa;
}
.breadcrumb-list{
	&{overflow: hidden;}
	.breadcrumb-arrows {font-size:16px; padding:10px 0;background: transparent;border-radius: 0;margin: 0;list-style: none;}
	.breadcrumb-arrows>li{ display: inline-block;}
	.breadcrumb-arrows>li strong{font-weight:400}
	.breadcrumb-arrows>li a{color: rgba($textmainColor,0.6);}
	.breadcrumb-arrows>li+li:before {	padding: 0 3px;margin-left:5px;color:#ccc;font-family: 'FontAwesome';content: "/\00a0";}
	@media(max-width:767px){
		.breadcrumb-arrows { padding:7px 0;font-size:12px;display: block;white-space: nowrap;overflow: scroll;}
		.breadcrumb-arrows>li+li:before {	padding: 0 2px;}
		.breadcrumb-arrows::-webkit-scrollbar {display: none;}
		.breadcrumb-arrows{-ms-overflow-style: none;scrollbar-width: none;}
	}
}
/* 2.3 - Pagination */
#pagination {
	margin: 30px 0;
	.pagination {
		margin: 0;padding: 0;justify-content: center;-webkit-justify-content: center;
		>li {
			display: inline-block;
			vertical-align: middle;
			&:not(:last-child) {margin-right: 8px;}
			>span,a{
				position: relative;
				float: left;
				line-height: 33px;
				text-decoration: none;
				color: #333e48;
				background-color: #fff;
				border: 1px solid #dee2e6;
				width: 35px;
				height: 35px;
				text-align: center;
				border-radius: 0 !important;
				font-weight: 600;
				padding: 0;
				cursor: pointer;
				&.current{
					background-color: var(--shop-color-main);
					border-color: var(--shop-color-main);
					color: #fff;
				}
			}
			>a:hover,>a:focus,>span:hover,>span:focus{
				z-index: 2;
				color: #fff;
				background-color: var(--shop-color-hover);
				border-color: var(--shop-color-hover);
			}
		}
	}
}
