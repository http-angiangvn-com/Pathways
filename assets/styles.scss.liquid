/*=======Index=======*/
.section-home-search {
	padding: 0;
	z-index: 9;
	position: relative;
	@media (max-width: 1300px){
		.container {padding: 0;}
	}
	.banner-search{
		display: flex;
		justify-content: center;
		align-items: center;
		padding: 10px 0;
		background: #fff;
		border-radius: 10px;
		margin-top: -45px;
		box-shadow: 0px 20px 80px 0px rgba(0, 0, 0, 0.05);
		position: relative;
		@media(max-width: 767px){
			flex-wrap: wrap;
			padding-top: 0;
		}
		@media(min-width: 768px){
			&:before {
				position: absolute;
				content: '';
				width: 3px;
				height: 40px;
				left: 0;
				z-index: 99;
				top: 50%;
				margin-top: -20px;
				background: var(--shop-color-main);
			}
		}

		.search-where{
			//flex: 0 0 calc(33.333% - 55px);
			flex: 0 0 calc(50% - 82.5px);
			display: flex;
			align-items: center;
			justify-content: center;
			border-right: 1px solid #e8edf1;
			@media(max-width: 767px){
				flex: 0 0 100%;
				padding: 15px 0;
				border: none;
				border-bottom: 1px solid #e8edf1;
				margin-bottom: 0;
			}

		}
		.search-person {
			flex: 0 0 calc(33.333% - 55px);
			display: flex;
			align-items: center;
			justify-content: center;
			border-right: 1px solid #e8edf1;
			@media(max-width: 767px){
				flex: 0 0 100%;
				padding: 15px 0;
				border: none;
				border-bottom: 1px solid #e8edf1;
				margin-bottom: 0;
			}
			#search_person {
				border: none;
				color: #757575;
				font-weight: 400;
				font-size: 15px;
				cursor: pointer;
			}
		}
		.search-when{
			//flex: 0 0 calc(33.333% - 55px);
			flex: 0 0 calc(50% - 82.5px);
			display: flex;
			align-items: center;
			justify-content: center;
			border-right: 1px solid #e8edf1;
			@media(max-width: 767px){
				flex: 0 0 100%;
				padding: 15px 0;
				border: none;
				border-bottom: 1px solid #e8edf1;
				margin-bottom: 0;
			}
			.t-date-info-title {
				color: #757575;
				font-size: 15px;
				top: 5px !important;
				@media(max-width: 991px){
					padding: 0px;
				}
			}
		}
		.search-btn {
			flex: 0 0 165px;
			text-align: right;
			padding-right: 10px;
			@media(max-width: 767px){
				padding: 0 15px;margin: 0;margin-top: 25px;margin-bottom: 15px;
			}
			button{
				padding: 0;text-align: center;
				border-radius: 0 8px 8px 0;
				padding: 20px 35px;
				font-size: 16px;
				@media(max-width: 767px){
					width: 100%;
					border-radius: 8px;
				}
			}
		}
		.search-icon {
			margin-right: 25px;
			font-size: 20px;
			color: var(--shop-color-main);
			font-weight: 400;
			width: 22px;
			text-align: center;
			@media(max-width: 991px){
				margin-right: 10px;
			}
		}
		.search-form{
			display: flex;
			justify-content: space-between;
			flex: 0 0 80%;
			position: relative;
			@media(max-width: 767px){
				flex: 0 0 calc(100% - 65px);
				.t-datepicker-day {
					left: 0;
					right: 0;
				}
			}
			label{margin: 0;}
			select{
				width: 100%;
				border: none;-moz-appearance: none;-webkit-appearance: none;appearance: none;outline: none;font-size: 15px;cursor: pointer;background: #fff;   
				color: #757575;
				font-weight: 400;
			}
			.search-arrow-value {
				svg {
					width: 18px;
					height: 18px;
					fill: #111111;
				}
			}    
			.t-datepicker {
				.t-check-in{width: 100%;
					.t-dates{padding: 0;height: 20px;}
				}
				.t-check-out{width: 0;.t-dates{display: none;}}
				.t-check-in .t-date-info-title, .t-check-out .t-date-info-title{top: 22px; left: 0;color: #4f5e71;opacity: 1;font-size: 15px;}
				.t-arrow-top {
					left: 20% !important;
				}
				.t-date-check-in span{display: none;}
			}
		}
		@media(max-width: 576px){
			flex-flow: column;border-radius: 15px;margin-top: 25px;
			>div{flex: 0 0 100% !important;width: 100%;margin-bottom: 15px;}
			.search-where{border-right: 0;}
			.search-when{padding-left: 0;}
			.search-form{
				.t-datepicker {
					.t-datepicker-days{width: 265px;right: auto;left: -35px;
						.t-table-wrap{width: 100%;margin-left: 0;}
					}
					.t-arrow-top{
						left: 5% !important;
					}
				}
			}
		}
	}
}

#home-slider {
	.slider-item {
		position: relative;
		.slide--image {
			img {
				width: 100% !important;
			}
		}
		.slide--body {
			position: absolute;
			width: 100%;
			height: 100%;
			left:0;
			top:0;
			display: flex;
			align-items: center;
			flex-wrap: wrap;
			padding-bottom: 50px;
			background: rgba(0, 0, 0, .2);
			.content-box {
				position: relative;
				display: block;
				z-index: 5;
				text-align: center;
				.special-text {
					position: relative;
					display: block;
					font-size: 40px;
					line-height: 48px;
					font-family: var(--secondary-font);
					color: var(--secondary-color);
					margin-bottom: 15px;
				}
				.te-title {
					font-size: 76px;
					line-height: 68px;
					font-weight: 700;
					color: #fff;
					margin-bottom: 30px;
					@media(max-width: 767px){
						font-size: 42px;
						line-height: 46px;
					}
					@media(max-width: 560px){
						font-size: 32px;
						line-height: 36px;
						margin-bottom: 15px;
					}
				}
				.te-text {
					font-size: 18px;
					line-height: 32px;
					color: #fff;
				}
			}
		}
	}
	@media(min-width: 992px) and (max-width: 1199px){
		.slider-item {
			.slide--body {
				.content-box {
					.special-text{font-size: 30px;}
					.te-title{font-size: 50px; line-height: 1.4;}
				}
			}
		}
	}
	@media(max-width: 991px){
		.slider-item {
			.slide--body {
				.content-box {
					.special-text{font-size: 25px;}
					.te-title{font-size: 40px; line-height: 1.4;}
				}
			}
		}
	}
	@media(max-width: 767px){
		.slider-item {
			.slide--body {
				.content-box {padding-top: 15px;
					.special-text{font-size: 18px;line-height: 1.4;margin: 0;}
					.te-title{font-size: 25px;margin-bottom: 10px;
						display: -webkit-box;
						-webkit-line-clamp: 1;
						-webkit-box-orient: vertical;
						overflow: hidden;
					}
					.te-text {
						font-size: 14px;
						line-height: 1.4;
						display: -webkit-box;
						-webkit-line-clamp: 3;
						-webkit-box-orient: vertical;
						overflow: hidden;
					}
				}
			}
		}
	}
}

.heading-box{
	margin-bottom: 30px;
	.heading-title {font-size: 40px;}
	.heading-desc {font-size: 16px;max-width: 550px;margin: 0 auto;}
	@media(max-width: 767px){
		.heading-title {font-size: 30px;}
		.heading-desc {font-size: 14px;}
	}
	@media(max-width: 480px){
		.heading-title {font-size: 24px;}
	}
}

.section-home-about_us {
	.about_us--left {
		display: flex;
		flex-direction: column;

		.about_us--body {
			.body-about-wrapper {
				margin: 0px 60px 10px 0px;
				@media (max-width: 767px){
					margin-right: 0px;
				}
			}
			.text-box {
				margin: 30px 0px 35px 0px;
				p {
					position: relative;
					color: #6a6a6a;
					font-weight: 400;
					margin: 0px;
					font-size: 16px;
					line-height: 1.7;
				}
			}
		}

		.about_us--body-travic_icon_box {
			.single-item {
				margin: 0px 60px 30px 0px;
				position: relative;
				display: block;
				padding-left: 80px;
				@media (max-width: 767px){
					margin-right: 0px;
				}
				.icon-box {
					position: absolute;
					display: inline-block;
					left: 0px;
					top: 0px;
					width: 60px;
					height: 60px;
					line-height: 60px;
					text-align: center;
					font-size: 25px;
					border-radius: 50%;
					color: var(--shop-color-main);
					&:before {
						position: absolute;
						content: '';
						width: 100%;
						height: 100%;
						left: 0px;
						top: 0px;
						border-radius: 50%;
						opacity: 0.1;
						background: var(--shop-color-main);
					}
				}
				h3 {
					display: block;
					font-size: 22px;
					line-height: 28px;
					font-weight: 600;
					margin-bottom: 12px;
					color: var(--secondary-title-color);
				}
				p {
					position: relative;
					color: #6a6a6a;
					font-weight: 400;
					font-size: 16px;
					margin: 0px;
					transition: all 500ms ease;
				}
			}
		}
	}
	.about_us--right {
		display: flex;
		flex-direction: column;
		.image-box {
			position: relative;
			display: block;
			@media (min-width: 560px){
				padding-bottom: 242px;
				padding-left: 319px;
				max-width: 630px;
				margin: 0 auto;
			}
			.image {
				border-radius: 10px;
				overflow: hidden;
				img {
					height: auto;
					max-width: 100%;
					border: none;
					border-radius: 0;
					box-shadow: none;
					border-radius: 10px;
					@media (max-width: 560px){
						width: 100% !important;
					}
				}
			}
			.image-1 img {
				width: 295px;
			}
			.image-2 {
				img {
					width: 309px;
				}
				@media (min-width: 560px){
					position: absolute;
					left: 0px;
					top: 135px;
				}
			}
			.image-3 {
				img {
					width: 205px;
				}
				@media (min-width: 560px){
					position: absolute;
					bottom: 0px;
					left: 320px;
				}
			}
		}
	}
	@media(min-width: 768px) and (max-width: 1199px){
		.about_us--left {
			.about_us--body-travic_icon_box {
				.single-item {
					h3{font-size: 20px;}
				}
			}
		}
	}
	@media(max-width: 991px){
		.about_us--left {
			.about_us--body {
				.body-about-wrapper{
					text-align: center;
				}
			}
		}
	}
	@media(max-width: 767px){
		.about_us--left {
			.about_us--body-travic_icon_box {
				.single-item {
					padding-left: 55px;
					h3{font-size: 18px;}
					p{font-size: 14px;}
					.icon-box{
						width: 45px;
						height: 45px;
						line-height: 45px;
						font-size: 22px;
					}
				}
			}
			.about_us--body {
				.text-box {
					p{font-size: 14px;}
				}
			}
		}
	}
}

.section-main-title {
	position: relative;
	display: block;
	margin-bottom: 50px;
	.sub-title {
		position: relative;
		display: block;
		font-size: 32px;
		line-height: 24px;
		font-family: var(--secondary-font);
		margin-bottom: 20px;
		color: var(--secondary-color);
	}
	.te-title {
		position: relative;
		display: block;
		font-size: 39px;
		line-height: 50px;
		font-weight: 600;
		color: var(--secondary-title-color);
		margin: 0px;
	}
	@media(min-width: 768px) and (max-width: 1199px){
		.sub-title {font-size: 25px;margin-bottom: 10px;}
		.te-title {font-size: 30px;line-height: 1.3;}
	}

	@media(max-width: 767px) {
		margin-bottom: 30px;
		.sub-title {font-size: 20px;margin-bottom: 10px;}
		.te-title {font-size: 26px;line-height: 1.3;}
	}
}

.section-home-why_choose {
	overflow: hidden;
	.why_choose--wrapper {
		flex-wrap: wrap;
		margin: 0 -35px;
		@media (max-width: 1024px){
			margin-top: -35px;
		}
		@media (max-width: 768px){
			margin: 0;
			margin-top: -35px;
		}
		.why_choose--item {
			width: 25%;
			text-align: center;
			padding: 0 35px;
			@media (max-width: 1024px){
				width: 50%;
				margin-top: 30px;
			}
			@media (max-width: 768px){
				width: 100%;
				padding: 0 15px;
			}
			.icon-box {
				position: relative;
				display: inline-block;
				font-size: 44px;
				margin-bottom: 25px;
				line-height: 0;
				img {
					width: 44px;
					height: 44px;
					object-fit: contain;
				}
			}
			.inner-box h3 {
				position: relative;
				display: block;
				font-size: 24px;
				line-height: 30px;
				margin-bottom: 13px;
				font-weight: 600;
			}
			.te-text {
				position: relative;
				color: #6a6a6a;
				font-weight: 400;
				font-size: 16px;
			}
		}
	}
	@media(min-width: 768px) and (max-width: 1199px){
		.why_choose--wrapper {
			.why_choose--item {
				.inner-box {
					h3{font-size: 20px;}
				}
			}
		}
	}
	@media(max-width: 767px){
		.why_choose--wrapper {
			.why_choose--item {
				.inner-box {
					h3{font-size: 18px;}
				}
				.te-text{font-size: 14px;}
			}
		}
	}
}

.section-home-tabs-collection {
	position: relative;
	background: #F6FBFF;
	&.lazyloaded {background-image: url('{{"home_collection_bg.jpg" | asset_url}}');}
	background-size: cover;
	background-position: center;
	.tabs-collection--box {
		position: relative;
		display: flex;
		align-items: flex-end;
		justify-content: space-between;
		gap: 0 30px;
		margin-bottom: 30px;
		@media (max-width: 991px){
			flex-wrap: wrap;
			justify-content: center;
			margin-bottom: 0;
		}
		.why_choose_title{
			@media (max-width: 991px){
				width: 100%;
				margin-bottom: 25px;
			}
			.section-main-title{
				margin-bottom: 0;
			}
		}
		.tab-btn-box {
			.tab-btns {
				position: relative;
				display: flex;
				align-items: center;
				flex-wrap: wrap;
				li {
					display: inline-block;
					font-size: 16px;
					line-height: 26px;
					font-weight: 500;
					color: var(--secondary-title-color);
					background: #fff;
					border-radius: 5px;
					cursor: pointer;
					padding: 7px 20px;
					margin-right: 10px;
					transition: all 500ms ease;
					@media (max-width: 991px){
						margin-bottom: 10px;
					}
					&.active-btn, &:hover {
						background: var(--shop-color-main);
						color: #fff;
					}
				}
			}
		}
	}
	.tabs-content {
		.tab {
			display: none;
			&.active-tab {
				display: block;
			}
			.tours-block {
				margin-top: 30px;
			}
		}
	}
	@media(max-width: 991px){
		.tabs-collection--box {
			.why_choose_title {
				.section-main-title{text-align: center;}
			}
			.tab-btn-box {
				.tab-btns {
					display: grid;
					&:not(.owl-loaded){display: flex;flex-flow: nowrap;overflow: hidden;
						li{flex: 0 0 100%;width: 100%;}
					}
					li{display: block; text-align: center;margin: 0;}
				}
			}
		}
		.tabs-content{
			.home-collection-list{
				margin: 0;padding-top: 15px;
				&:not(.owl-loaded){display: flex;flex-flow: nowrap;overflow: hidden;
					.tours-block{flex: 0 0 100%;width: 100%;}
				}
				.tours-block{
				padding: 0;
				}
			}
		}
		.owl-carousel .owl-nav{
