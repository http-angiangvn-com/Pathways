/*=======Product loop=======*/
.product-loop {
	background-color: #fff;
	border-radius: 10px;
	padding: 8px;
	-webkit-box-shadow: 0 0 50px rgba(0, 0, 0, 0.05);
	box-shadow: 0 0 50px rgba(0, 0, 0, 0.05);
	height: 100%;
	.product-inner{
		.proloop-image{
			position: relative;
			.product--image{
				border-radius: 8px 8px 0 0;
				position: relative;overflow: hidden;
				img{border-radius: 8px 8px 0 0;-webkit-transition: all .5s ease-in-out;transition: all .5s ease-in-out;}
				&:hover{
					img{-webkit-transform: scale(1.1);transform: scale(1.1);}
				}
			}
			.pro-sale, .pro-featured, .pro-soldout{
				position: absolute;
				color: #fff;
				font-weight: 600;
				font-size: 12px;
				z-index: 2;
				padding: 2px 8px;
				border-radius: 3px;
			}
			.pro-sale{top: 10px;left: 10px;background: red;}
			.pro-soldout{bottom: 10px;right: 10px;background: #9d9d9d;}
			.pro-featured{top: 10px;right: 10px;background: #05a528;}
		}
		.proloop-detail {
			padding: 15px 5px;
			.proloop--vendor{
				border-radius: 5px;
				display: -ms-inline-flexbox;
				display: inline-flex;
				background-color: rgba($mainColor, 0.05);
				gap: 4px;
				-webkit-box-align: center;
				-ms-flex-align: center;
				align-items: center;
				padding: 3px 9px;
				-webkit-transition: all .4s ease;
				transition: all .4s ease;
				svg{margin-top: -1px;color: var(--shop-color-main);}
			}
			.proloop--title{font-size: 18px;display: -webkit-box;-webkit-line-clamp: 3;-webkit-box-orient: vertical;overflow: hidden;min-height: 65px;}
			.list-info {
				>div{
					.info-title{font-size: 14px;color: #a0a0a0;}
				}
			}
			.price-loop {/*flex: 0 0 56%;*/
				.proloop--price {
					margin-bottom: 5px;
					min-height: 46px;
					.price{font-weight: 600;font-size: 18px;}
					.person{font-size: 14px;color: #565;}
					.price-del {
						color: #a0a0a0;
						text-decoration: line-through;
						font-weight: 300;
						font-size: 14px;display: block;
						.person{color: #a0a0a0;}
					}
				}
			}
			.guide-loop{flex: 0 0 42%;
				.guide-info {
					display: flex;
					align-items: center;
					gap: 5px;
					.guide-img {
						flex: 0 0 20px;
						img{border-radius: 50%;}
					}
					.guide-name {
						font-weight: 600;
						display: -webkit-box;
						-webkit-line-clamp: 1;
						-webkit-box-orient: vertical;
						overflow: hidden;
					}
				}
			}
		}
	}
	@media(max-width: 767px){
		height: auto;
		.product-inner{
			.proloop-detail{
				.proloop--title{font-size: 16px;min-height: auto;display: block;}
			}
		}
	}
}
