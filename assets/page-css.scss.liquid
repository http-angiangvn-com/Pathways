.list-blogs-latest {counter-reset: post-ranking;}
.list-blogs-latest .item-article {border-bottom: 1px #efefef dotted;padding: 15px 0;margin: 0;}
.list-blogs-latest .item-article:last-child{border-bottom:none;}
.list-blogs-latest .item-article .post-image {width: 100px;padding-right: 10px;float: left;position: relative;}
.list-blogs-latest .item-article .post-image:before {position: absolute;
	content: counter(post-ranking);
	top: calc(50% - 16px);
	left: -16px;
	border: 2px solid #fff;
	height: 28px;
	width: 28px;
	line-height: 26px;
	text-align: center;
	border-radius: 100%;
	background: var(--shop-color-main);
	color: #fff;
	font-size: 12px;counter-increment: post-ranking;
