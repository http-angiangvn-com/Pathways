	.boxscroll {
		overflow-y: scroll;
		max-height: 330px;
		-webkit-overflow-scrolling: touch;
		-ms-scroll-chaining: none;
		overscroll-behavior: none;
		margin-right: -20px;
		padding-right: 20px;padding-top: 12px;
		&::-webkit-scrollbar {width:4px;	height: 3px; }
