}
.list-blogs-latest .item-article .post-content {width: calc(100% - 100px);float: left;}
.list-blogs-latest .item-article .post-content h3 {margin: 0 0 5px;font-size: 13px;font-weight: 500;}
.list-blogs-latest .item-article .post-content h3 a{
	white-space: initial;
	overflow: hidden;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	display: -webkit-box;
}
