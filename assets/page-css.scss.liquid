			&.swal-button--cancel {border: 1px solid #000;background-color: #000;color:#fff;}
			&.swal-button--confirm {border: 1px solid #ec0b0b;background-color: #ec0b0b;color:#fff;}
		}
	}

	&.swal-cart-remove .swal-button--confirm {background: var(--shop-color-button);color:#ffffff;}
