{%comment%}
{%- if settings.toolbar_bottom_selected == 'toolbar_style_1' or settings.toolbar_bottom_selected == 'toolbar_style_2' -%}
{%- assign classtoolbar = 'has-toolbar' -%}
{%-endif-%}
{%endcomment%}

<footer class="mainFooter  {{classtoolbar }}">
  <div class="footer-background lazyload"></div>
	<div class="footer-container">
		<div class="footer-expand-collapsed ">
			<div class="container ft-top">
				<div class="row justify-content-spacebetween">
					{% if settings.footer_about_show %}
					<div class="col-lg-3 col-md-12 col-12 widget-footer widget-about">
						<div class="content-footer block-collapse row">
							<div class="col-lg-12 col-md-12 col-12">
                {% if settings.use_logo_ft %}
								<div class="ft-logo">
									<a href="{{shop.url}}"  itemprop="url">
										<img itemprop="logo" src="{{ 'logo-footer.png' | asset_url}}" alt="{{ shop.name }}" class="img-responsive logoimg lazyload"/>
									</a>	
								</div>
                {% endif %}
								<div class="address-footer">
									<ul>
										{%- if settings.footer-address != empty -%}<li class="contact-1">{{settings.footer-address}}</li>{%- endif -%}
										{%- if settings.footer-email != empty -%}<li class="contact-2">{{settings.footer-email}}</li>{%- endif -%}
										{%- if settings.footer-tell != empty -%}<li class="contact-3">{{settings.footer-tell}}</li>{%- endif -%}
									</ul>
								</div>
								{% if settings.footer_logobct_show %}
								<div class="logo-footer">
									<a href="{{settings.footer_logobct_link}}" target="_blank" rel="noreferrer">
										<img class="lazyload" data-src="{{'footer_logobct_img.png' | asset_url  }}" 
												 src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII="
												 alt="{{settings.footer_logobct_alt}}" >
									</a>
								</div>
								{% endif %}
							</div>
						</div>
					</div>
					{% endif %}
					{% if settings.footer_linklist1_show %}
					<div class="col-lg-3 col-md-12 col-12 widget-footer footer-link">
						<h4 class="title-footer">{{settings.footer_linklist1_title}}</h4>
						<div class="content-footer block-collapse">				
							<ul class="footerNav-link">
								{%for link in linklists[settings.footer_linklist1_menu].links%}
								<li class="item">
									<a href="{{link.url}}" title="{{link.title}}">{{link.title}}</a>
								</li>
								{%endfor%}	
							</ul>		
						</div>
					</div>
					{% endif %}
					{% if settings.footer_linklist2_show %}
					<div class="col-lg-3 col-md-12 col-12 widget-footer footer-link">
						<h4 class="title-footer">{{settings.footer_linklist2_title}}</h4>
						<div class="content-footer block-collapse">				
							<ul class="footerNav-link">
								{%for link in linklists[settings.footer_linklist2_menu].links%}
								<li class="item">
									<a href="{{link.url}}" title="{{link.title}}">{{link.title}}</a>
								</li>
								{%endfor%}	
							</ul>			
						</div>
					</div>
					{% endif %}
      		{% if settings.gallery_show %}
					<div class="col-lg-3 col-md-12 col-12 widget-footer">
						<h4 class="title-footer">{{settings.gallery_footer_title}}</h4>
						<div class="content-footer block-collapse">	
							<div class="footer-gallery">
								<ul class="gallery-icons">
									{%- for i in (1..6) -%}
									{%- capture show_gallery -%}gallery_{{i}}_show{%- endcapture -%}
									{%- capture icon_gallery -%}gallery_{{i}}_img.png{%- endcapture -%}
									{%- capture alt_gallery -%}gallery_{{i}}_alt{%- endcapture -%}
									{%- if settings[show_gallery] -%}
									<li><img class="lazyload" data-src="{{ icon_gallery | asset_url }}" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" alt="{{settings[alt_gallery]}}" title="{{settings[alt_gallery]}}"></li>
									{%- endif -%}
									{%- endfor -%}
								</ul>
							</div>	
							
						</div>
	
					</div>
          {% endif %}
				
				</div>
			</div>
      <div class="footer-bottom">
  			<div class="container">
          <div class="footer-copyright-social">	
    			{%if settings.footer_copyright_show%}
    			<div class="footer-copyright">
            {%if settings.footer_copyright %}
            <p>{{settings.footer_copyright}}</p>
            {%else%}
            <p>Copyright Â© {{ "now" | date: "%Y" }} <a href="{{ shop.url }}"> {{ shop.name }}</a>, Inc. All Rights Reserved</p>
            {%endif%}
    			</div>
    			{%endif%}
          {% if settings.social_footer_show %}
          <div class="content-footer">	
            <ul class="footerNav-social">
              {%-if settings.social_link_facebook != blank -%}<li><a href="{{settings.social_link_facebook}}" target="_blank" rel="noopener" title="Facebook" aria-label="Facebook"><i class="fa fa-facebook"></i></a></li>	{%-endif-%}
                {%-if settings.social_link_youtube != blank -%}<li><a href="{{settings.social_link_youtube}}" target="_blank" rel="noopener" title="Youtube" aria-label="Youtube"><i class="fa fa-youtube-play"></i></a></li>	{%-endif-%}
                {%-if settings.social_link_instagram != blank -%}<li><a href="{{settings.social_link_instagram}}" target="_blank" rel="noopener" title="Instagram" aria-label="Instagram"><i class="fa fa-instagram"></i></a></li>	{%-endif-%}
