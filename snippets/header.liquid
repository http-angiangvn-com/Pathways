                            <path d="M9.1923882 8.39339828l7.7781745-7.7781746 1.4142136 1.41421357-7.7781746 7.77817459 7.7781746 7.77817456L16.9705627 19l-7.7781745-7.7781746L1.41421356 19 0 17.5857864l7.7781746-7.77817456L0 2.02943725 1.41421356.61522369 9.1923882 8.39339828z" fill-rule="evenodd"></path>
                          </svg>
                        </span>
                        <span class="count-holder">
                          <span class="count">{{ cart.item_count }}</span>
                        </span>
                      </span>
                    </a>
                    <span class="box-triangle">
                      <svg viewBox="0 0 20 9" role="presentation">
                        <path d="M.47108938 9c.2694725-.26871321.57077721-.56867841.90388257-.89986354C3.12384116 6.36134886 5.74788116 3.76338565 9.2467995.30653888c.4145057-.4095171 1.0844277-.40860098 1.4977971.00205122L19.4935156 9H.47108938z" fill="#ffffff"></path>
                      </svg>
                    </span>
                  </div>
                </div>
              {% endif %}
              {% if settings.header_acc_title != '' %}
                <div class="header-action-item header-action_account">
                  <div class="header-action_text">
                    <a class="header-action__link"  href="/account" id="site-account-handle" aria-label="{{ settings.header_acc_title }}" title="{{ settings.header_acc_title }}">
                        <i class="fa fa-user-o" aria-hidden="true"></i> {{ settings.header_acc_title }}
                    </a>
                  </div>

                </div>
              {% endif %}
            </div>
          </div>
        </div>
      </div>
    </div>
  </header>
</div>