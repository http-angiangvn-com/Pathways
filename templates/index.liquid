{%- if settings.show_home_testimonials -%}
<section class="section-home-testimonials lazyload">
	<div class="container">
		<div class="top_visited_title">
			<div class="section-main-title text-center">
				<span class="sub-title te-subtitle">{{settings.home_testimonials_title}}</span>
				<h2 class="te-title">{{settings.home_testimonials_main_title}}</h2>
			</div>
		</div>
		<div class="owl-carousel owl-theme nav-style-one owl-dots-style">
			{% for i in (1..6) %}
			{% capture home_testimonials_show %}home_testimonials_show_{{i}}{% endcapture %}
			{%- if settings[home_testimonials_show] -%}
			{% capture home_testimonials_avatar %}home_testimonials_avatar_{{i}}.jpg{% endcapture %}
			{% capture home_testimonials_name %}home_testimonials_name_{{i}}{% endcapture %}
			{% capture home_testimonials_job %}home_testimonials_job_{{i}}{% endcapture %}
			{% capture home_testimonials_content %}home_testimonials_content_{{i}}{% endcapture %}
			{% capture home_testimonials_rating %}home_testimonials_rating_{{i}}{% endcapture %}
			<div class="testimonial-block-one">
				<div class="inner-box te-testimonial">
					<div class="rating">
						{% for j in (1..5) %}
						{% if j <= settings[home_testimonials_rating] %}<i class="fa fa-star"></i>{%else%}<i class="fa fa-star-o"></i>{%endif%}
					{% endfor %}
				</div>
				<div class="author-box">
					<figure class="author-thumb">
						<img width="60" height="60" class="lazyload" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-src="{{home_testimonials_avatar | asset_url}}" class="post-image" alt="{{settings[home_testimonials_name] | strip_html}}" />
					</figure>
					<h3 class="te-title">{{settings[home_testimonials_name]}}</h3>
					<span class="designation te-designation">{{settings[home_testimonials_job]}}</span>
				</div>
				<p class="te-text">{{settings[home_testimonials_content]}}</p>
			</div>
		</div>
		{%- endif -%}
		{%endfor%}
	</div>
	</div>
</section>
{%- endif -%}

{%- if settings.show_home_blog -%}
<section class="section-home-blog">
	<div class="container">
		<div class="top_visited_title">
			<div class="section-main-title text-center">
				<span class="sub-title te-subtitle">{{settings.home_blog_title}}</span>
				<h2 class="te-title">{{settings.home_blog_main_title}}</h2>
			</div>
		</div>
		<div class="owl-carousel owl-theme owl-dots-style">
			{% for article in blogs[settings.home_blog_main_select].articles %}
			<div class="news-block">
				<div class="news-block-one">
					<div class="inner-box te-blog">
						<figure class="image-box">
							<a href="{{article.url}}">
								<img class="" src="{{article.image.src | product_img_url: 'grande'}}" class="post-image" />
							</a>
						</figure>
						<div class="lower-content">
							<ul class="post-info mb_15">
								<li class="te-category">
									<a href="{{article.url}}" rel="category tag">{{article.blog.title}}</a>
								</li>
								<li class="te-meta"><i class="fa fa-calendar" aria-hidden="true"></i>{{ article.published_at | date: '%d Tháng %m, %Y' }}</li>
							</ul>
							<h3 class="te-title">
								<a href="{{article.url}}">{{article.title}}</a>
							</h3>
						</div>
					</div>
				</div>
