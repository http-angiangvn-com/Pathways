{%- if settings.show_home_collection -%}
<div class="section-home-tabs-collection lazyload">
	<div class="container">

		<div class="tabs-collection--box">
			<div class="why_choose_title">
				<div class="section-main-title">
					<span class="sub-title te-subtitle">{{settings.home_collection_title}}</span>
					<h2 class="te-title">{{settings.home_collection_main_title}}</h2>
				</div>
			</div>
			<div class="tab-btn-box">
				<ul class="tab-btns tab-buttons clearfix">
					{%assign count = -1 %}
					{% for i in (1..7) %}
					{% capture home_collection_tab_show %}show_home_collection_tab_{{i}}{% endcapture %}
					{% if settings[home_collection_tab_show] or forloop.first %}
					{% capture home_collection_title_tab %}home_collection_title_tab_{{i}}{% endcapture %}
					{% capture home_collection_col_tab_btn %}home_collection_col_tab_{{i}}{% endcapture %}
					{%assign count = count | plus: 1 %}
					<li class="tab-btn{% if forloop.first %} active-btn{%endif%}" data-index="tab-mb-{{count}}" data-handle="/collections/{{settings[home_collection_col_tab_btn]}}" data-tab="#tab-{{i}}">{{settings[home_collection_title_tab]}}</li>
					{%endif%}
					{% endfor %}
				</ul>
			</div>
		</div>

		<div class="tabs-content">
			{% for i in (1..7) %}
			{% capture home_collection_tab_show %}show_home_collection_tab_{{i}}{% endcapture %}
			{% if settings[home_collection_tab_show] or forloop.first %}
			{% capture home_collection_col_tab %}home_collection_col_tab_{{i}}{% endcapture %}
			{% assign homeCollection = settings[home_collection_col_tab] %}
			{% assign homeCollectionLimit = settings.home_collection_limit %}
			<div class="tab{% if forloop.first %} collection-loaded active-tab{%endif%}" id="tab-{{i}}">
				<div class="home-collection-list row clearfix ">
					{%- if forloop.first -%}
					{% for product in collections[homeCollection].products limit: homeCollectionLimit %}
					<div class="col-lg-3 col-md-6 col-sm-12 tours-block">
						{% include 'product-loop' %}
					</div>
					{%- endfor -%}
					{%- else -%}
					{% for i in (1..homeCollectionLimit) %}
					<div class="col-lg-3 col-md-6 col-sm-12 tours-block">
						{% include 'product-loop-skeleton' %}
					</div>
					{%endfor%}
					{%- endif -%}
				</div>
			</div>
			{%endif%}
			{% endfor %}

		</div>
	</div>
</div>
{%- endif -%}

