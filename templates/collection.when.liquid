{%layout none%}
{%paginate collection.products by 50 %}
{%assign list_date = ''%}
{%for product in collection.products%}
{%assign tag_date = ''%}
{% for tag in product.tags %}
{% if tag contains 'tag_date_' %} 
{%assign tag_date = tag | remove:'tag_date_' | append: ','%}
{%assign list_date = list_date | append: tag_date %}
{% endif %}
{% endfor %}
{%endfor%}
{{list_date}}
{%endpaginate%}